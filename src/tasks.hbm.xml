<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.model.tasks" table="users">
        <id name="taskId" type="int" column="taskId" >
            <generator class="increment"/>
        </id>
        <property name="userId">
            <column name="userId"/>
        </property>
        <property name="taskName">
            <column name="taskName"/>
        </property>
        <property name="desc">
            <column name="desc"/>
        </property>
        <property name="taskCurrentStatus">
            <column name="taskCurrentStatus"/>
        </property>
	</class>
</hibernate-mapping>